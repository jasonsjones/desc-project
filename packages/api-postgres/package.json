{
    "name": "@desc/api-postgres",
    "version": "0.1.0",
    "description": "RESTful json API for the DESC project written in typescript and backed by a postgres data store",
    "main": "index.js",
    "scripts": {
        "build": "tsc",
        "clean": "rm -rf dist coverage",
        "dev": "ts-node-dev --respawn src/index.ts",
        "lint": "eslint src/**/*.ts",
        "start": "node dist/src/index.js",
        "test": "NODE_ENV=testing jest --color --runInBand",
        "test:e2e": "NODE_ENV=testingE2E yarn dev"
    },
    "author": "Jason S. Jones",
    "license": "MIT",
    "dependencies": {
        "bcryptjs": "2.4.3",
        "body-parser": "1.19.0",
        "cookie-parser": "1.4.5",
        "cors": "2.8.5",
        "dotenv": "8.2.0",
        "express": "4.17.1",
        "jsonwebtoken": "^9.0.0",
        "morgan": "^1.10.0",
        "nodemailer": "6.5.0",
        "passport": "0.4.1",
        "passport-local": "1.0.0",
        "pg": "8.5.1",
        "reflect-metadata": "0.1.13",
        "typeorm": "0.2.31",
        "uuid": "8.3.2"
    },
    "devDependencies": {
        "@types/bcryptjs": "2.4.2",
        "@types/body-parser": "1.19.0",
        "@types/cookie-parser": "1.4.2",
        "@types/cors": "2.8.10",
        "@types/dotenv": "8.2.0",
        "@types/express": "4.17.11",
        "@types/jest": "26.0.20",
        "@types/jsonwebtoken": "8.5.0",
        "@types/morgan": "1.9.2",
        "@types/nodemailer": "6.4.0",
        "@types/passport": "1.0.6",
        "@types/passport-local": "1.0.33",
        "@types/supertest": "2.0.10",
        "@types/uuid": "8.3.0",
        "@typescript-eslint/eslint-plugin": "4.17.0",
        "@typescript-eslint/parser": "4.17.0",
        "jest": "26.6.3",
        "supertest": "6.1.3",
        "ts-jest": "26.5.3",
        "ts-node-dev": "1.1.6",
        "typescript": "4.2.3"
    },
    "engines": {
        "node": ">=12.20.0 <16.0.0",
        "yarn": ">=1.16.0"
    }
}
